desc:4ch Filters - 1st order Butterworth Highpass Filter (cookdsp) (Lossius)

import cookdsp.jsfx-inc
import lossius-cookdsp-mods.jsfx-inc

slider1:3500<20,10000, 0.1>Cutoff Frequency In Hz

in_pin:1
in_pin:2
in_pin:3
in_pin:4

out_pin:1
out_pin:2
out_pin:3
out_pin:4


/********************************************************
*
* Init
*
********************************************************/
@init

filt1.buthp1(slider1);
filt2.buthp1(slider1);
filt3.buthp1(slider1);
filt4.buthp1(slider1);


/********************************************************
*
* Sliders
*
********************************************************/
@slider

filt1.buthp1_set_freq(slider1);
filt2.buthp1_set_freq(slider1);
filt3.buthp1_set_freq(slider1);
filt4.buthp1_set_freq(slider1);


/********************************************************
*
* Sample processing
*
********************************************************/
@sample

spl0 = filt1.buthp1_do(spl0);
spl1 = filt2.buthp1_do(spl1);
spl2 = filt3.buthp1_do(spl2);
spl3 = filt4.buthp1_do(spl3);